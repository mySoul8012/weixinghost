/* miniprogram/pages/index/index.wxss */
.panel{
  position:relative;
}
.panel-child{
  padding: 0;
  margin: 0;
  z-index: 0;
}
.images-title{
  width: 500px;
  height: 300px;
}
.panel-title{
  position: absolute;
  z-index: 2;
  width: 100%;
  bottom: 30%;
  opacity: 0.8;
  text-align: center;
}
.bottom {
  height: 50px;
}
.loading{
  position:relative;
  left: 45%;
}
.loading-none{
  display: none;
}
